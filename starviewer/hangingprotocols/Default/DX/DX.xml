<?xml version="1.0" encoding="UTF-8"?>
<!-- 
    Aquest hanging protocol està pensat perquè s'apliqui sobre
    estudis DX amb més d'una sèrie, on la sèrie primera i segona
    ocupen el primer i segon visor
-->
<hangingProtocol name="DX">
<hangingProtocolName>DX</hangingProtocolName>
<numberScreens>1</numberScreens>
<protocol>DX</protocol>
<imageSet identifier="1">
<type>image</type>
<imageNumberInPatientModality>0</imageNumberInPatientModality>
</imageSet>
<imageSet identifier="2">
<type>image</type>
<imageNumberInPatientModality>1</imageNumberInPatientModality>
</imageSet>
<displaySet identifier="1">
<imageSetNumber>1</imageSetNumber>
<position>0.0\1.0\0.5\0.0</position>
</displaySet>
<displaySet identifier="2">
<imageSetNumber>2</imageSetNumber>
<position>0.5\1.0\1.0\0.0</position>
</displaySet>
</hangingProtocol>